{
  "expo": {
    "name": "Shopping List",
    "description": "Shopping List",
    "slug": "shopping-list",
    "privacy": "public",
    "primaryColor": "#55ab2b",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "hooks": {
      "postPublish": [
        {
          "file": "./sentry",
          "config": {
            "organization": "devinim",
            "project": "shopping-list"
          }
        }
      ]
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devinim.shoppinglist"
    },
    "android": {
      "package": "com.devinim.shoppinglist",
      "versionCode": 1
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#55ab2b"
    }
  }
}